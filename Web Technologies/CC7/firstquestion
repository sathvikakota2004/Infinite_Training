<!DOCTYPE html>
<html>
<head>
    <title>JavaScript Multiplication and Division</title>
    <script>
        function Multiply() {
            var num1 = parseInt(document.getElementById("fnum").value);
            var num2 = parseInt(document.getElementById("snum").value);
            var result = num1 * num2;
            document.getElementById("result").innerText = "The Result Is: " + result;
        }

        function Division() {
            var num1 = parseInt(document.getElementById("fnum").value);
            var num2 = parseInt(document.getElementById("snum").value);
            if (num2 === 0) {
                document.getElementById("result").innerText = "Cannot divide by zero!";
            } else {
                var result = num1 / num2;
                document.getElementById("result").innerText = "The Result Is:" + result;
            }
        }
    </script>
</head>
<body>
    First Number: <input type="number" id="fnum"><br><br>
    Second Number: <input type="number" id="snum"><br><br>
    <button onclick="Multiply()">Multiply</button>
    <button onclick="Division()">Division</button>
    <p id="result"></p>
</body>
</html>
